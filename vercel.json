{
  "version": 2,
  "builds": [
    {
      "src": "serve/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/client/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/serve/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/client/dist/index.html"
    }
  ]
}

