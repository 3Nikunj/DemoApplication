{
  "version": 2,
  "builds": [
    {
      "src": "serve/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/serve/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "client/client/dist/index.html"
    }
  ]
}